<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>fr</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>org.dojoserverfaces.tests.resources.Messages</base-name>
			<var>message</var>
		</resource-bundle>
	</application>
	<!--  To be used as a wrapper for ExternalContext-->
	<factory>
		<external-context-factory>org.dojoserverfaces.tests.application.TestExternalContextFactory</external-context-factory>
	</factory>
	
</faces-config>