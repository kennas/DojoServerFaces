	<context-param>
		<param-name>javax.faces.FACELETS_LIBRARIES</param-name>
		<param-value>/WEB-INF/dojoserverfaces_test.taglib.xml</param-value>
	</context-param>

	<context-param>
		<description>path to the dojo library resources
		</description>
		<param-name>dojoserverfaces.dojo.location</param-name>
		<param-value>//eclipse-dojotoolkit/dojotoolkit/</param-value>
	</context-param>
	<servlet>
		<servlet-name>Faces Servlet</servlet-name>
		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Faces Servlet</servlet-name>
		<url-pattern>*.faces</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.faces</welcome-file>
	</welcome-file-list>
</web-app>